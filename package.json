{
  "name": "react-lap",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.4.0",
    "react-loader-spinner": "^5.1.5",
    "react-player": "^2.10.1",
    "react-router-dom": "^5.3.3",
    "react-scripts": "5.0.0",
    "use-debounce": "^8.0.1"
  },
  "scripts": {
    "watch:css": "cd ./tailwind_init & postcss ./tailwind.css -o ../src/styles.css -w",
    "build:css": "cd ./tailwind_init && postcss ./tailwind.css -o ../src/styles.css",
    "start": "react-scripts start",
    "build": "npm run build:css && react-scripts build && echo '/* /index.html 200' | cat > build/_redirects",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^10.4.7",
    "postcss-cli": "^9.1.0",
    "tailwindcss": "^3.1.3"
  }
}
